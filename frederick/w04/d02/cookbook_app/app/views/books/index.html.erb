<h1>Books</h1>

<%= link_to "Add Book", new_book_path %>
<% @books.each do |book| %>
<h3><%= link_to book.title, book_path(book) %></h3>
<p><%= book.chef %></p>
<p><%= link_to 'Show', book_path(book) %> | <%= link_to 'Edit', edit_book_path(book) %> | <%= link_to 'Destroy', book_path(book),
	method: :delete, data: { confirm: 'Are you sure?' } %></p>
	<% end %>
