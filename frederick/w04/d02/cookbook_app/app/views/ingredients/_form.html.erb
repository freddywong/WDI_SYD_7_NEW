<%= form_for @ingredient do |f| %>

<div class="form_group">
	<%= f.label :name %>
	<%= f.text_field :name %>
</div>

<div class="form_group">
	<%= f.label :measurement %>
	<%= f.text_field :measurement %>
</div>

<div class="form_group">
	<%= f.label :cost %>
	<%= f.text_field :cost %>
</div>

<div class="form_group">
	<%= f.label :image %>
	<%= f.url_field :image %>
</div>

<div class="form-group">
	<%= f.submit %>
</div>

<% end %>