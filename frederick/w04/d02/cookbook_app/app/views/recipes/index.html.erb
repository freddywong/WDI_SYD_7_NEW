<h1>Recipes</h1>
<% @recipes.each do |recipe| %>
<h3><%= link_to recipe.name, recipe_path(recipe) %></h3>
<p><%= link_to 'Show', recipe_path(recipe) %> | <%= link_to 'Edit', edit_recipe_path(recipe) %> | <%= link_to 'Destroy', recipe_path(recipe),
	method: :delete, data: { confirm: 'Are you sure?' } %></p>
	<% end %>